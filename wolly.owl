:Secondario rdf:type owl:Class ;
            rdfs:subClassOf :Ruolo ;
            rdfs:comment "Secondario - Personaggio in secondo piano rispetto al principale"^^xsd:string ;
            rdfs:label "Secondario"@it .


###  http://www.semanticweb.org/albertolillo/wolly#Sentimentale
:Sentimentale rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :genere ;
                                    owl:hasValue "Sentimentale"
                                  ] ;
              rdfs:subClassOf :Genere ;
              rdfs:comment "Sentimentale - Opera cinematografica in cui vi sono storie d'amore che si concentrano sulla passione, sull'emozione e sul coinvolgimento affettivo dei personaggi principali e il viaggio compiuto dal loro sentimento d'amore che li porta attraverso il corteggiamento o il matrimonio."^^xsd:string ;
              rdfs:label "Sentimentale"@it .


###  http://www.semanticweb.org/albertolillo/wolly#Serie
:Serie rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :contiene ;
                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :Stagione
                           ] ;
       rdfs:subClassOf :Cartoon ;
       rdfs:comment "Serie - Insieme di stagioni"^^xsd:string ;
